{
    "name": "laravel/github-webhooks",
    "description": "Laravel package for handling GitHub webhooks",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "github",
        "webhooks",
        "package"
    ],
    "authors": [
        {
            "name": "Balesco",
            "email": "balehormo25@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/routing": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Laravel\\GitHubWebhooks\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravel\\GitHubWebhooks\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\GitHubWebhooks\\GitHubWebhooksServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
